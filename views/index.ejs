                <a href="/descartados?id=<%=dadosVencidos[n]._id%>" class="btn btn-success">Descartado</a>
                <a href="/alterar?id=<%=dadosVencidos[n]._id%> " class="btn btn-warning text-light">Alterar</a>
                <a href="/excluir?id=<%=dadosVencidos[n]._id%>" class="btn btn-danger">Excluir</a> 
                <%}%>
            </div>
          </div>
        </div>

        <div class="accordion-item">
          <h2 class="accordion-header" id="flush-headingThree">
            <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#flush-collapseThree" aria-expanded="false" aria-controls="flush-collapseThree">
              Consumidos (<%=dadosExcluidos.length%>)
            </button>
          </h2>
          <div id="flush-collapseThree" class="accordion-collapse collapse" aria-labelledby="flush-headingThree" data-bs-parent="#accordionFlushExample">
            <div class="accordion-body">
              <% for (var n=0; n<dadosExcluidos.length; n++){%>
                <div class="accordion-body">Nome: <%=dadosExcluidos[n].nome%> Data: <%=dadosExcluidos[n].data.getDate()+1%>-<%=dadosExcluidos[n].data.getMonth()+1%>-<%=dadosExcluidos[n].data.getFullYear()%></div>
                <a href="/desfazer?id=<%=dadosExcluidos[n]._id%>" class="btn btn-warning text-light">Desfazer</a>
                <a href="/excluir?id=<%=dadosExcluidos[n]._id%>" class="btn btn-danger">Excluir</a> 
                <%}%>
            </div>
          </div>
        </div>

        <div class="accordion-item">
          <h2 class="accordion-header" id="flush-headingFour">
            <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#flush-collapseFour" aria-expanded="false" aria-controls="flush-collapseFour">
